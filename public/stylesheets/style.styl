body
  margin: 0
  padding: 0
  font: 16px "Lucida Grande", Helvetica, Arial, sans-serif
h1,h2
  margin: 0
  padding: 5px
h1
  background-color: rgb(0,0,0)
  color: rgb(255,255,255)
  font-size: 1.5em
h2
  background-color: rgb(50,50,50)
  color: rgb(255,255,255)
  font-size: 1em
section
  h1
    border-top: 1px solid rgb(255,255,255)
    font-size: 1em
p
  strong
    font-weight: bolder
a
  color: #00B7FF
