    link(rel='stylesheet', href='css/style.css', type='text/css', media='screen', title='no title', charset='utf-8')
    script(src='js/jquery.js', type='text/javascript', charset='utf-8')
    script(src='js/ticker.js', type='text/javascript', charset='utf-8')
    script(type='text/javascript')
      $(function(){
      $(".ticker li").newsTicker({type:"longitudinally"});
      });
    title imakita[イマキタ]
  body
    header.header.clearfix
      h1.logo
        a(href='/')
          img(src='img/logo.png', alt='imakita イマキタ', width='115', height='45')
    section.note-body
      .note-body-top
        p.event-title-text.num Event Title
        h2.event-title 第2回デザイナー x エンジニアハッカソン#p4dhack
