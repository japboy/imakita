        p.members-arrival 24
        p.members-all 35
      .btn-imakita
        form
          input.form-btn-imakita(type='submit', name='commit', value='今、ここまできた！')
      .ticker-wrapper
        .ticker#ticker
          ul
            li
              span.time 17:46
              span.name
                a(href='#') @ken_c_lo
              | が到着しました
            li
              span.time 18:22
              span.name
                a(href='#') @satococoa
              | があと15km地点にまできました
            li
              span.time 19:04
              span.name
                a(href='#') @japboy
              | が到着しました
            li
              span.time 19:35
              span.name
                a(href='#') @machida
              | があと200km地点にまできました
      ul.accounts
        li.arrive
          a(href='#')
            img(src='img/dummy_ico_twi.jpg', alt='ken_c_lo')
            span.screen-name @japboy
            span.kita-time
              | 15:02
              span.kita-time-small kita
        li.ontheway
          a(href='#')
            img(src='img/dummy_ico_twi.jpg', alt='ken_c_lo')
            span.screen-name @ken_c_lo
            span.distance
              | あと
              span.num 15km
        li.nodata
          a(href='#')
            img(src='img/dummy_ico_twi.jpg', alt='ken_c_lo')
